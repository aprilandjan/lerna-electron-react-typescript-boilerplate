{
  "name": "app-main",
  "version": "1.0.0",
  "private": true,
  "author": "aprilandjan",
  "description": "Electron app resources for the Electron app",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "cross-env START_HOT=1 NODE_ENV=development electron -r @babel/register ./src/index.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js --colors",
    "start": "cross-env NODE_ENV=production electron ./dist/main.prod.js",
    "package": "electron-builder build --publish never",
    "package-all": "electron-builder build -mwl",
    "package-ci": "electron-builder --publish always",
    "package-linux": "electron-builder build --linux",
    "package-win": "electron-builder build --win --x64"
  },
  "license": "MIT",
  "dependencies": {
    "devtron": "^1.4.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^4.0.0",
    "electron-updater": "^4.2.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "electron": "7.1.4",
    "cross-env": "^6.0.3",
    "@babel/register": "^7.7.4",
    "electron-builder": "^22.2.0"
  },
  "main": "./dist/main.prod.js",
  "build": {
    "productName": "ElectronReact",
    "appId": "org.develar.ElectronReact",
    "files": [
      "./dist",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "play-electron",
      "repo": "play-electron",
      "private": false
    }
  }
}
