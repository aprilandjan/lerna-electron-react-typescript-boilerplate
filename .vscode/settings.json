{
  "files.associations": {
    ".commitlintrc": "json",
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".stylelintrc": "json",
    ".dockerignore": "ignore",
    ".eslintignore": "ignore"
  },
  "typescript.tsdk": "./node_modules/typescript/lib",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    "bower_components": true,
    "dist": true,
    "dll": true,
    "release": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "yarn-error.log": true,
    "test/**/__snapshots__": true
  },
  "nodeDebugProcessPicker.autoAttach": true,
  "nodeDebugProcessPicker.match": ["!**/node_modules/electron/**/*"]
}
